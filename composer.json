{
  "name": "plenta/contao-mimir",
  "type": "contao-bundle",
  "description": "Send exceptions within your Contao installation through the Notification Center",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Plenta.io",
      "homepage": "https://plenta.io"
    }
  ],
  "funding": [{
    "type": "github",
    "url": "https://github.com/sponsors/plenta"
  }],
  "support":{
    "issues": "https://github.com/plenta/contao-mimir/issues",
    "source": "https://github.com/plenta/contao-mimir",
    "docs": "https://plenta.io/contao-erweiterungen/"
  },
  "require": {
    "php": "^8.1",
    "contao/conflicts": "@dev",
    "contao/core-bundle": "^4.13.0 || ^5.1",
    "doctrine/dbal": "^2.10.1 || ^3.2.0",
    "symfony/http-client": "^5.4 || ^6.0"
  },
  "require-dev": {
    "contao/manager-bundle": "^5.1",
    "phpunit/phpunit": "^9.5.10",
    "contao/test-case": "^4.4.0",
    "symfony/phpunit-bridge": "v5.3.8",
    "friendsofphp/php-cs-fixer": "^3.2"
  },
  "extra": {
    "contao-manager-plugin": "Plenta\\Mimir\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "Plenta\\Mimir\\": "src"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "contao-components/installer": true,
      "contao/manager-plugin": true,
      "contao-community-alliance/composer-plugin": true,
      "php-http/discovery": true
    }
  }
}
